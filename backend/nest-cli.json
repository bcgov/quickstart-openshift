{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "builder": "swc",
    "typeCheck": true,
    "deleteOutDir": true,
    "assets": [
      {
        "include": "../generated/prisma/**/*",
        "outDir": "dist",
        "watchAssets": true
      }
    ],
    "watchAssets": true
  }
}
